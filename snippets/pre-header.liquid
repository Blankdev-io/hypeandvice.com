{% if template contains 'index' %}
<link rel="preload" as="image" href="https://cdn.shopify.com/s/files/1/0061/4167/3590/files/collage-final_1_1800x.jpg?v=1648728112">
<link rel="preload" as="image" href="https://cdn.shopify.com/s/files/1/0061/4167/3590/files/IMG_9917_77d63630-ec05-4eea-9b1a-22580410048a_500x.jpg?v=1649239701">
{% endif %}

{% if template contains 'product' %}
<link rel="preload" as="image" href="{{ product.featured_image | img_url: '800x' }}">
{% endif %}

{% if template contains 'collection' %}
  {% if collection.image != blank %}
  	<link rel="preload" as="image" href="{{ collection.image | img_url: '1800x' }}">
  {% else %}
  	<link rel="preload" as="image" href="{{ collection.products.first.featured_image | img_url: '500x' }}">
  {% endif %}
{% endif %}

<link rel="preconnect" href="//fonts.shopifycdn.com/" crossorigin>
<link rel="preconnect" href="//cdn.shopify.com/" crossorigin>
<link rel="preconnect" href="//v.shopify.com/" crossorigin>
<link rel="dns-prefetch" href="//{{shop.domain}}" crossorigin>
<link rel="dns-prefetch" href="//{{shop.permanent_domain}}" crossorigin>
